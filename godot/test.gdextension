[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../rust-backend/target/debug/librust_backend.so"
linux.release.x86_64 =   "res://../rust-backend/target/release/librust_backend.so"
windows.debug.x86_64 =   "res://../rust-backend/target/debug/rust_backend.dll"
windows.release.x86_64 = "res://../rust-backend/target/release/rust_backend.dll"
macos.debug =            "res://../rust-backend/target/debug/librust_backend.dylib"
macos.release =          "res://../rust-backend/target/release/librust_backend.dylib"
macos.debug.arm64 =      "res://../rust-backend/target/debug/librust_backend.dylib"
macos.release.arm64 =    "res://../rust-backend/target/release/librust_backend.dylib"
